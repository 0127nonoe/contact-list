<!DOCTYPE html>
<html>
<head>
  <title>Contact List</title>
  <link rel="stylesheet" type="text/css" href="/styles.css">
</head>
<body>
  <h1>Contact List</h1>
  <table border="1">
    <thead>
      <tr>
        <th>Profile Picture</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Age</th>
        <th>Email</th>
        <th>ID</th>
        <th>Phone</th>
        <th>Label</th>
        <th>Organization</th>
        <th>Country</th>
       
        
        
      </tr>
    </thead>
    <tbody>
      <% formattedContacts.forEach(function(contact) { %>
        <tr>
            <td>
            
                <img src="<%= contact.profilePicture %>" alt="Profile Picture" width="100">
              </td>
          <td><%= contact.firstName %></td>
          <td><%= contact.lastName %></td>
          <td><%= contact.age %></td>
          <td><%= contact.email %></td>
          <td><%= contact.id %></td>
          <td><%= contact.phone %></td>
          <td><%= contact.label %></td>
          <td><%= contact.organization %></td>
          <td><%= contact.country %></td>
          
          
       
      <% }); %>
    </tbody>
  </table>
</body>
</html>
